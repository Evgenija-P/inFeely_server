{
	"trailingComma": "none",
	"useTabs": true,
	"semi": false,
	"tabWidth": 4,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"arrowParens": "avoid",
	"printWidth": 100,
	"endOfLine": "auto",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^components/(.*)$",
		"^hooks/(.*)$",
		"^types/(.*)$",
		"^services/(.*)$",
		"^assets/(.*)$",
		"^utils/(.*)$",
		"^config/(.*)$",
		"^providers/(.*)$",
		"^navigation/(.*)$",
		"^api/(.*)$",
		"^store/(.*)$",
		"^data/(.*)$",
		"^styles/(.*)$",
		"^../(.*)$",
		"^./(.*)$",
		"^[./]",
		"^[a-z]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderParserPlugins": ["typescript", "jsx"],
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
